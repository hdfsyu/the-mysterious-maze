function _init()--init
poke(0x5f5c,5)
poke(0x5f5d,4)
	spr_flags()
	text_setup()
	player()
	game_win=false
	game_over=false
	sfx(00) --im not gonna enable my trash music...
end
function _draw()--sprites
	cls()
	if(not game_over) then
		draw_map()
		draw_player()
		draw_text()
		if (btn(‚ùé)) show_inventory()
	else
		draw_win_lose()
	end
end
function _update()--update
	if(not game_over) then
		if(not active_text) then
			update_sprites()
			move_player()
			check_win_lose()
		end
	else
		if(btnp(‚ùé)) extcmd("reset")
	end
end
-->8
--sprite flags and drawing
function spr_flags()--sprite flags
	--timers
	timer=0
	anim_ftime=26
	--end of timers
	coins={4}
	wall={7,9}
	door={9}
	sanim1={11}
	sanim2={12}
	win={13}
	lose={11}
	text={14}
	gold={15}
	teleport={16}
end
function update_sprites()
	if(timer<0) then
		toggle_tiles()
		timer=anim_ftime
	end
	timer-=1
end
function draw_map()
	mapx=flr(p.x/16)*16
	mapy=flr(p.y/16)*16
	camera(mapx*8, mapy*8)
	map(0,0,0,0,128,64)
end
function is_tile(tile_type,x,y)
	tile=mget(x,y)
	for i=1,#tile_type do
		if(tile==tile_type[i]) return true
	end
	return false
end
function can_move(x,y)
	return not is_tile(wall,x,y)
end
function swap_coin(x,y)
	tile=mget(x,y)
	mset(x,y,tile+4)
end
function swap_door(x,y)
	tile=mget(x,y)
	mset(x,y,tile+1)
end
function unswap_tile(x,y)
	tile=mget(x,y)
	mset(x,y,tile-1)
end
function swap_tile(x,y)
	tile=mget(x,y)
	mset(x,y,tile+1)
end
function swap_gold(x,y)
	tile=mget(x,y)
	mset(x,y,tile-7)
end
function swap_sign(x,y)
	tile=mget(x,y)
	mset(x,y,tile-6)
end
function collect_coin(x,y)
	p.coins+=1
	swap_coin(x,y)
	sfx(2)
end
function open_door(x,y)
	p.coins-=1
	swap_door(x,y)
	sfx(3)
end
function get_gold(x,y)
	p.gold+=5
	swap_gold(x,y)
	sfx(2)
end
function read_sign(x,y)
	sfx(2)
	swap_sign(x,y)
end
function teleport_maze()
	p.x=16
	p.y=7
end
-->8
--player
function player()
	p={}
	p.x=16
	p.y=7
	p.anim1=5
	p.anim2=6
	p.coins=0
	p.gold=0
end
function draw_player()
	spr(p.anim1,p.x*8,p.y*8)
end
function move_player()--maps buttons and changes x and y pos of player
	newx=p.x
	newy=p.y
	if(btnp(‚¨ÖÔ∏è)) newx-=1
	if(btnp(‚û°Ô∏è)) newx+=1
	if(btnp(‚¨ÜÔ∏è)) newy-=1
	if(btnp(‚¨áÔ∏è)) newy+=1
	interact(newx,newy)
	if(can_move(newx, newy)) then
		p.x=mid(0,newx,127)
		p.y=mid(0,newy,63)
	else
		sfx(1)
	end
end
function interact(x,y)
	if(is_tile(text,x,y)) then
		active_text=get_text(x,y)
	end
	if(is_tile(coins,x,y)) then
		collect_coin(x,y)
	elseif(is_tile(door,x,y) and p.coins>11) then
		open_door(x,y)
	elseif(is_tile(gold,x,y)) then
		get_gold(x,y)
	elseif(is_tile(text,x,y)) then
		read_sign(x,y)
	end
end

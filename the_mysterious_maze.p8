--text
function text_setup()
	texts={}
	add_text(16,5,"get through the maze\nand collect all the coins!")
	add_text(16,15,"left or right? you choose.\nbeware of the spikes!")
	add_text(41,11,"turn back. now.")
end
function add_text(x,y,message)
	texts[x+y*128]=message
end
function get_text(x,y)
	return texts[x+y*128]
end
function draw_text()
	if(active_text) then
		textx=mapx*8+4
		texty=mapy*8+48
		rectfill(textx,texty,textx+119,texty+31,7)
		print(active_text,textx+4,texty+4,5)
		print("üÖæÔ∏è to close",textx+4,texty+23,6)
	end
	if(btnp(üÖæÔ∏è)) active_text=nil
end

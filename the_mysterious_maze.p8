--inventory
function show_inventory()
	invx=mapx*8+29
	invy=mapy*8+8
	cleft=p.coins-31
	rectfill(invx,invy,invx+77,invy+30,0)
	print("inventory",invx+7,invy+4,7)
	print("coins left  "..cleft,invx+12,invy+14,10)
	print("gold "..p.gold,invx+12,invy+20,9)
end
-->8
--animated tiles
function toggle_tiles()
	for x=mapx,mapx+15 do
		for y=mapy,mapy+15 do
			if(is_tile(sanim1,x,y)) then
				swap_tile(x,y)
				sfx(4)
			elseif(is_tile(sanim2,x,y)) then
				unswap_tile(x,y)
				sfx(4)	
			end
		end
	end
end
